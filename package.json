{
  "name": "component-library",
  "version": "1.0.0",
  "description": "this is a component library for static react components",
  "main": "dist/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c",
    "tsc": "tsc --noEmit",
    "test": "COMPILE_COMMON=true mocha 'test/*.{js,jsx,ts,tsx}' 'test/**/*.{js,jsx,ts,tsx}'",
    "storybook": "storybook dev -p 6006 -s dist --ci",
    "build-storybook": "storybook build",
    "format": "prettier --loglevel warn --write \"**/*.{js,json,jsx,md,ts,tsx,html}\"",
    "format:check": "prettier --check \"**/*.{js,json,jsx,md,ts,tsx,html}\""
  },
  "keywords": [
    "library",
    "react",
    "component"
  ],
  "author": "M.Yusuf",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@storybook/addon-essentials": "^7.0.7",
    "@storybook/addon-interactions": "^7.0.7",
    "@storybook/addon-links": "^7.0.7",
    "@storybook/blocks": "^7.0.7",
    "@storybook/react": "^7.0.7",
    "@storybook/react-webpack5": "^7.0.7",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.16.3",
    "@types/react": "^18.2.0",
    "@types/sinon": "^10.0.14",
    "eslint": "^8.39.0",
    "mocha": "^10.2.0",
    "prettier": "^2.8.8",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.62.1",
    "sinon": "^15.0.4",
    "storybook": "^7.0.7",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "engines": {
    "node": "^18.0.0",
    "npm": "1.20.0"
  }
}